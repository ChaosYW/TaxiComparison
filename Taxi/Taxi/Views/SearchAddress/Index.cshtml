@model Taxi.Models.SearchAddressViewModel
    @{
        ViewBag.Title = "Search";
    }



    <h2>Price Compare</h2>

    <p class="text-success">@ViewBag.StatusMessage</p>



    <div class="container">
        <form>
            <div class="form-group">
                <label for="usr">Name:</label>
                <input type="text" class="form-control" id="iptFrom">
            </div>
            <div class="form-group">
                <label for="pwd">Password:</label>
                <input type="text" class="form-control" id="iptTo">
            </div>
            <div class="form-group">
                <button type="button" class="btn btn-primary btn-sm">Button 1</button>
            </div>
        </form>
    </div>


    <div class="container">
        <div style="width:80%;height:80%;">
            <div id="googleMap" style="width:800px;height:800px;"></div>
        </div>
    </div>
    <script>
        function myMap() {
            var mapProp = {
                center: new google.maps.LatLng(40.737209, -73.819660),
                zoom: 12,
            };
            var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
        }
    </script>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDZ2b3ucOfoCQrm3eqU8qC-1EKP_P2Sh2M&callback=myMap"></script>


