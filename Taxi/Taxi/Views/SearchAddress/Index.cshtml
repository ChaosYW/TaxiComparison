@model Taxi.Models.SearchAddressViewModel
@{
    ViewBag.Title = "Search";
}



<h2>Price Compare</h2>

<p class="text-success">@ViewBag.StatusMessage</p>



<div class="container">
    <form>
        <div class="form-group">
            <label for="usr">From:</label>
            <input id="iptFrom" type="text" class="form-control" id="iptFrom">
        </div>
        <div class="form-group">
            <label for="pwd">To:</label>
            <input id="iptTo" type="text" class="form-control" id="iptTo">
        </div>
        <div class="form-group">
            <button id="btSearch" type="button" class="btn btn-primary btn-sm">Search</button>
        </div>
    </form>
</div>


<div class="container">
    <div style="width:80%;height:80%;">
        <div id="googleMap" style="width:800px;height:800px;"></div>
    </div>
</div>

@Scripts.Render("~/Scripts/SearchAddress.js")
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDZ2b3ucOfoCQrm3eqU8qC-1EKP_P2Sh2M&callback=myMap"></script>

